{
  "name": "opendota-core",
  "description": "Open source Dota data platform",
  "version": "6.0.1",
  "license": "GPL-3.0",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "NODE_ENV=test test/run.sh",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "ncu": "npm-check-updates -a",
    "webpack": "webpack -p --bail",
    "watch": "webpack --watch",
    "mocha": "NODE_ENV=test mocha",
    "build": "npm install && npm run webpack",
    "lint": "eslint . --ext .js,.jsx --fix",
    "rediskeys": "redis-cli keys '*' | cut -d':' -f1 | sort | uniq -c",
    "resetpicks": "redis-cli keys 'picks_*' | xargs redis-cli del",
    "resetrankings": "redis-cli keys 'hero_rankings*' | xargs redis-cli del",
    "dockerbuild": "sudo docker build -t odota/core .",
    "reparse": "cat matches.csv | xargs -n1 node tasks/getMatch.js"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/odota/core"
  },
  "dependencies": {
    "JSONStream": "1.2.1",
    "async": "2.1.2",
    "big-number": "0.4.0",
    "body-parser": "1.15.2",
    "bootstrap": "3.3.7",
    "bootswatch": "3.3.7",
    "braintree": "1.41.0",
    "bull": "1.0.0",
    "c3": "0.4.11",
    "cal-heatmap": "3.6.1",
    "cassandra-driver": "3.1.5",
    "chance": "1.0.4",
    "compression": "1.6.2",
    "connect-multiparty": "2.0.0",
    "connect-redis": "3.1.0",
    "cookie-session": "2.0.0-alpha.1",
    "datatables": "1.10.12",
    "datatables-bootstrap3-plugin": "0.5.0",
    "dota2": "4.1.0",
    "dota2-minimap-hero-sprites": "2.0.0",
    "dotaconstants": "3.4.0",
    "dotenv": "2.0.0",
    "express": "4.14.0",
    "express-session": "1.14.0",
    "flag-icon-css": "2.3.0",
    "font-awesome": "4.6.3",
    "heatmap.js": "2.0.0",
    "http-proxy": "1.15.2",
    "jade": "1.11.0",
    "jquery": "3.1.1",
    "knex": "0.12.6",
    "moment": "2.15.2",
    "multer": "1.2.0",
    "numeral": "1.5.3",
    "passport": "0.3.2",
    "passport-steam": "1.0.6",
    "pg": "6.0.3",
    "pg-query-stream": "1.0.0",
    "pm2": "2.0.18",
    "qtip2": "3.0.3",
    "redis": "2.6.2",
    "request": "2.76.0",
    "request-progress": "2.0.1",
    "select2": "4.0.3",
    "select2-bootstrap-theme": "0.1.0-beta.8",
    "sentiment": "2.0.0",
    "simple-vdf": "1.1.0",
    "steam": "1.4.0",
    "steam-user": "3.13.0",
    "stripe": "4.11.0",
    "wordcloud": "1.0.3"
  },
  "devDependencies": {
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "expose-loader": "0.7.1",
    "file-loader": "0.9.0",
    "istanbul": "0.4.4",
    "json-loader": "0.5.4",
    "mocha": "3.1.2",
    "nock": "9.0.0",
    "style-loader": "0.13.1",
    "supertest": "2.0.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1"
  },
  "engines": {
    "node": "6.9.0"
  }
}
